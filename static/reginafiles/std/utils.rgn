class StringBuilder {
    string = []
    fun add(s) {
        string.add(s)
    }
    fun toString(sep="") {
        return string.joinToString(sep)
    }
}

// The maximum and the minimum are inclusive
fun rndInt(min, max) {
    min = min.ceil()
    max = max.floor() + 1
    return (rnd() * (max - min) + min).floor()
}

fun rndDouble(min, max) {
    return (rnd() * (max - min) + min)
}
